      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class rdf:about="http://www.co-ode.org/ontologies/pizza/pizza.owl#FruitTopping"/>
        </owl:someValuesFrom>
        <owl:onProperty>
          <owl:InverseFunctionalProperty rdf:about="http://www.co-ode.org/ontologies/pizza/pizza.owl#hasTopping"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
